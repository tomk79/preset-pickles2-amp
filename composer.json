{
    "name": "tomk79/preset-pickles2-amp",
    "description": "Pickles 2 で AMP 対応のサイトを制作するサンプルです。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0" ,
        "broccoli-html-editor/broccoli-module-fess": "0.1.*",
        "broccoli-html-editor/broccoli-module-plain-html-elements": "0.1.*",
        "pickles2/px-fw-2.x": "^2.0",
        "pickles2/px2-dec": "^2.0",
        "pickles2/px2-multitheme": "^2.0",
        "pickles2/px2-path-resolver": "^2.0",
        "pickles2/px2-px2dthelper": "^2.0",
        "pickles2/px2-remove-attr": "^2.0",
        "pickles2/px2-sitemapexcel": "^2.0",
        "tomk79/px2-amp-convert": "^0.1",
        "tomk79/px2-publish-for-multi-device": "^0.1"
    },
    "require-dev": {
    },
    "autoload": {
        "files": [
            "px-files/php/user_agent.php"
        ]
    },
    "extra": {
        "px2package": {
            "name": "Get start AMP with \"Pickles 2\" !",
            "type": "project",
            "path": ".px_execute.php",
            "path_homedir": "px-files/"
        }
    }
}
